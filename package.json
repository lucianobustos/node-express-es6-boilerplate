{
  "name": "node-express-es6-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Node.js projects with lint, transpile and test configurations.",
  "main": "src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config ./build/webpack.api.base.babel.js",
    "clean": "./node_modules/.bin/rimraf ./dist",
    "lint": "./node_modules/.bin/eslint --color --format stylish ./src --fix",
    "prebuild": "npm run clean",
    "start": "./node_modules/.bin/babel-node src/index.js",
    "test": "./node_modules/.bin/jest --config jest.config.json",
    "test-watch": "npm run test -- --watch",
    "test-coverage": "npm run test -- --coverage",
    "swagger": "./node_modules/.bin/swagger-jsdoc -d  ./src/swagger/index.js -o openapi.json"
  },
  "author": "Suvankar Kundu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suvankar-kundu/node-express-es6-boilerplate.git"
  },
  "keywords": [
    "nodejs",
    "boilerplate",
    "es6",
    "es2015",
    "babel",
    "express",
    "lint"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^1.1.6",
    "json-loader": "^0.5.7",
    "rimraf": "^2.6.2",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.3",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "express": "4.16.2",
    "mongodb": "^3.5.2",
    "mongoose": "5.0.9",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "passport-jwt": "3.0.1",
    "redis": "^3.0.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  }
}
